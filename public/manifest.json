{
  "manifest_version": 3,
  "name": "千面书签",
  "version": "1.0.0",
  "description": "千面书签的定位是市场上第一个真正将 AI 驱动的个人效率与 企业级实时协作 完美结合的新标签页插件",
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "书签管理",
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["bookmarks", "storage", "tabs"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline';"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.html", "*.js", "*.css", "*.png", "*.jpg", "*.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
