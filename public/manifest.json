{
  "manifest_version": 3,
  "name": "千面书签｜智能个人导航与企业链接工作台",
  "version": "1.0.1",
  "description": "一键定制你的新标签页：书签树形管理、快捷链接、企业链接工作台、主题切换、待办与日历提醒，数据本地存储，开箱即用，立即提升效率。",
  "icons": {
    "16": "marklogo.png",
    "32": "marklogo.png",
    "48": "marklogo.png",
    "128": "marklogo.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "书签管理",
    "default_icon": {
      "16": "marklogo.png",
      "32": "marklogo.png",
      "48": "marklogo.png",
      "128": "marklogo.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["bookmarks", "storage"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline';"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.html", "*.js", "*.css", "*.png", "*.jpg", "*.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
